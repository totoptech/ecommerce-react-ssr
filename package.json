{
  "name": "williams-sonoma",
  "version": "1.0.0",
  "description": "Williams Sonoma",
  "engines": {
    "node": "10.14.2"
  },
  "main": "index.js",
  "scripts": {
    "start": "node build/bundle.js",
    "build:prod": "rm -rf build && rm -rf public && webpack  --config webpack.server.js && webpack -p --config webpack.client.prod.js --mode production",
    "heroku-postbuild": "npm install && npm run build:prod",
    "webpack-prod": "webpack -p --progress --config webpack.client.prod.js --mode production",
    "dev": "npm run build-server && npm-run-all --parallel dev:*",
    "build-server": "webpack --config webpack.server.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "precommit": "pretty-quick --staged"
  },
  "author": "David Zhen",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.41",
    "@material-ui/system": "^4.9.1",
    "@sentry/browser": "^5.12.1",
    "axios": "0.19.2",
    "clsx": "^1.1.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^3.1.0",
    "concurrently": "5.1.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "material-ui-flat-pagination": "^4.1.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "4.1.5",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-images": "^1.1.7",
    "react-lazy-load-image-component": "^1.4.0",
    "react-redux": "7.1.3",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "2.1.2",
    "styled-components": "^5.0.1",
    "terser-webpack-plugin": "^2.3.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-dev-server": "3.10.1",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2"
  },
